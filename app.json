{
  "expo": {
    "name": "Hy Todo List",
    "description": "This project is really great.",
    "slug": "reactnative",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#48ADFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.hytodolist.hy",
      "supportsTablet": true,
    },
    "android": {
      "package": "com.hytodolist.hy"
    }
  }
}
